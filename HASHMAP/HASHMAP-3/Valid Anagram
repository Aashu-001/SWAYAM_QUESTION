class Solution {
public:
    bool isAnagram(string s, string t) {
        vector<int>count1(26,0);
        vector<int>count2(26,0);
        if(s.size()!=t.size()){
            return false;
        }
        for(int i=0;i<s.size();i++){
            int index = s[i]-'a';
            count1[index]++;
        }
         for(int i=0;i<t.size();i++){
            int index = t[i]-'a';
            count2[index]++;
        }
        for(int i=0;i<26;i++){
             if(count1[i]!=count2[i]){
                return false;
             }
        }
        return true;
    }
};
