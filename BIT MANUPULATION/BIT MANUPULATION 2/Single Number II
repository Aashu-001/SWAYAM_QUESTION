class Solution {
public:
    int singleNumber(vector<int>& nums) {
        //max 32 bits can be present 
        int n= nums.size();
        int val =0;
        for(int i =0 ;i< 32;i++){
           int sum =0 ;
           for(int num :nums){
            sum = sum + (num>>i &1);
           }
           sum = sum %3;
          val = val|sum<<i;
        }
        return val;
        //count the no of set bit at the ith bit 

    }
};
