class Solution {
  public:
    vector<int> bfs(vector<vector<int>> &adj) {
        // code here
        queue<int>q;
        vector<int>ans;
        q.push(0);
        int n = adj.size();
        vector<int>visited(n,0);
        visited[0] = 1;
        while(!q.empty()){
            int node = q.front();
            q.pop();
            ans.push_back(node);
            for(int i =0 ;i < adj[node].size();i++){
               if(!visited[adj[node][i]]){
                   q.push(adj[node][i]);
                   visited[adj[node][i]] = 1;
               }
            }
        }
        return ans;
    }
};
