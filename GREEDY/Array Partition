class Solution {
public:
    int arrayPairSum(vector<int>& nums) {
        priority_queue<int>pq;
        for(int i =0 ;i<nums.size();i++){
            pq.push(nums[i]);
        }
        int sum =0;
        while(!pq.empty()){
           int num1 = pq.top();
           pq.pop();
           int num2 = pq.top();
           pq.pop();
           sum = sum+min(num1,num2);
        }
      return sum;
    }
};
