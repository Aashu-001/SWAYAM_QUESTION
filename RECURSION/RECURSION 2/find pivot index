class Solution {
public:
    int pivotIndex(vector<int>& nums) {
       int rSum =0; 
       for(int i =0;i<nums.size();i++){
        rSum =rSum + nums[i];
       } 
       int lSum =0;
       for(int i =0;i<nums.size();i++){
         rSum = rSum - nums[i];
         if(lSum == rSum){
            return i;
         }
         lSum = lSum + nums[i];
       }
       return -1;
    }
};
