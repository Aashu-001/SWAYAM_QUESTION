class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int val = -1;
        int count =0;
        for(int index =0;index < nums.size();index++){
            if(count == 0 ||nums[index] == val){
                count++;
                val = nums[index];
            }
           
            else{
                count--;
            }
        }
        return val;
    }
};
