class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        int n=nums.size();
        vector<int> ans;
        int num=1;
        ans.push_back(nums[0]);
        for(int i=1;i<n;i++){
            if(nums[i]!=nums[num-1]){
               nums[num]=nums[i];
               num++;
            }
        }
           
        return num;
    }
};
