class Solution {
public:
        bool getEval(char beg,char end){
        if( beg == '(' && end == ')'){
            return true;
        }
        else if( beg == '{' && end == '}'){
            return true;
        }
        else if( beg == '[' && end == ']'){
            return true;
        }
        return false;
     };
    bool isValid(string s) {
        stack<char>st;
        for(int i=0;i<s.length();i++){
            if(s[i]=='('||s[i]=='['||s[i]=='{'){
                st.push(s[i]);
            }
            else if(s[i]==']'||s[i]=='}'||s[i]==')'){
                if(st.empty()||!getEval(st.top(),s[i])){
                    return false;
                }
                else{
                    st.pop();
                }
            }
        }
        return st.empty();
    }

};
